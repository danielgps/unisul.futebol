<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>futebol</display-name>
	<welcome-file-list>
		<welcome-file>/pages/public/index.jsp</welcome-file>
	</welcome-file-list>
	<jsp-config>
		<jsp-property-group>
			<url-pattern>/pages/public/*</url-pattern>
			<include-prelude>/resources/template/header.jsp</include-prelude>
			<include-coda>/resources/template/footer.jsp</include-coda>
		</jsp-property-group>
		<jsp-property-group>
			<url-pattern>/pages/private/*</url-pattern>
			<include-prelude>/resources/template/header-admin.jsp</include-prelude>
			<include-coda>/resources/template/footer-admin.jsp</include-coda>
		</jsp-property-group>
	</jsp-config>
</web-app>